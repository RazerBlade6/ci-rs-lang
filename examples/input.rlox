var time = clock();

var max = input("Enter the number of fibonnaci numbers to calculate: ");
max = parse(max, "number");

fun fib(n) {
    if (n <= 1) {
        return n;
    }

    return fib(n - 2) + fib(n - 1);
}

print("fibonnaci numbers are: ");

for (var i = 0; i < max; i = i + 1) {
    print(fib(i));
}

print("Time taken is: " + (clock() - time) + "s");